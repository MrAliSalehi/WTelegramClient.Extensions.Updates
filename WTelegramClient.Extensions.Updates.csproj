<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>net6.0;net6.0-windows;net7.0;net7.0-windows;net8.0;net8.0-windows</TargetFrameworks>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<Authors>Qwxp</Authors>
		<Description>Extensions over WtelegramClient For Dealing With Updates</Description>
		<Copyright> Apache-2.0</Copyright>
		<PackageProjectUrl>https://github.com/MrAliSalehi/WTelegramClient.Extensions.Updates</PackageProjectUrl>
		<PackageIcon>logo.png</PackageIcon>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<Version>1.0.2</Version>
	</PropertyGroup>

	<ItemGroup>
		<None Include="D:\Projects\C#\Library\WTelegramClient\Extensions\Updates\WTelegramClient.Extensions.Updates\README.md" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="WTelegramClient" Version="3.6.3"/>
	</ItemGroup>

	<ItemGroup>
		<None Update="logo.png">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
	</ItemGroup>
</Project>
